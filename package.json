{
  "name": "terminal",
  "title": "Atom Terminal",
  "main": "./src/terminal",
  "repository": "https://github.com/atom-ide-community/terminal",
  "bugs": {
    "url": "https://github.com/atom-ide-community/terminal/issues/new/choose"
  },
  "version": "0.0.0",
  "description": "The xterm based terminal for Atom",
  "keywords": [
    "terminal",
    "xterm",
    "term",
    "console",
    "shell",
    "emulator",
    "pty",
    "tty",
    "comspec",
    "command-line",
    "bash",
    "sh",
    "powershell",
    "cmd"
  ],
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "atomTestRunner": "./spec/custom-runner",
  "atomTranspilers": [
    {
      "transpiler": "atom-ts-transpiler",
      "glob": "{!(node_modules)/**/,}*.ts?(x)"
    }
  ],
  "license": "MIT",
  "engines": {
    "atom": ">=1.50.0 <2.0.0"
  },
  "providedServices": {
    "platformioIDETerminal": {
      "description": "Run commands and open terminals.",
      "versions": {
        "1.1.0": "providePlatformIOIDEService"
      }
    },
    "terminal": {
      "description": "Change the terminal.",
      "versions": {
        "1.0.0": "provideTerminalService"
      }
    }
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-ts-transpiler": "^1.5.2",
    "fs-extra": "^9.0.1",
    "node-pty-prebuilt-multiarch": "^0.9.0",
    "uuid": "^8.3.1",
    "whatwg-url": "^8.4.0",
    "which": "^2.0.2",
    "xterm": "4.9.0",
    "xterm-addon-fit": "0.4.0",
    "xterm-addon-ligatures": "^0.3.0",
    "xterm-addon-web-links": "0.4.0",
    "xterm-addon-webgl": "0.9.0"
  },
  "devDependencies": {
    "@types/atom": "1.40.5",
    "@types/fs-extra": "^9.0.3",
    "@types/jasmine": "^3.6.1",
    "@types/node": "^14.14.6",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/uuid": "^8.3.0",
    "@types/whatwg-url": "^8.0.0",
    "@types/which": "^1.3.2",
    "atom-jasmine3-test-runner": "^5.1.7",
    "eslint": "^7.12.1",
    "eslint-config-atomic": "^1.5.1",
    "npm-check-updates": "9.2.4",
    "prettier": "^2.1.2",
    "temp": "^0.9.2",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "atom --test spec",
    "tsc": "tsc -p tsconfig.json",
    "bump": "ncu -u -x coffeescript"
  },
  "deserializers": {
    "TerminalModel": "deserializeTerminalModel"
  }
}
